  },
  "dependencies": {
    "ajv": "^8.12.0",
    "dayjs": "^1.11.10",
    "ofetch": "^1.3.3"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "typescript": "^4.9.0",
    "@types/node": "^20.10.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}