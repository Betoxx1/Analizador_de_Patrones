{
  "name": "@analizador-patrones/ingest",
  "version": "1.0.0",
  "type": "module",
  "description": "Data ingestion and validation for analizador-patrones - CORREGIDO",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsx src/index.ts",
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
